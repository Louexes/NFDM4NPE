#!/bin/bash

#SBATCH --partition=gpu_a100
#SBATCH --gres=gpu:1 
#SBATCH --cpus-per-task=9
#SBATCH --gpus=1
#SBATCH --job-name=dirichlet_nfdm
#SBATCH --ntasks=1
#SBATCH --time=4:59:00
#SBATCH --mem=8000M
#SBATCH --output=dirichlet_nfdm_%A.out

