module load 2024

# Activate environment
source venv/bin/activate

# Run your code
python main.py --save_path result --dataset dirichlet_multinomial --device 0 --data_type=iid --epochs=2000 --model=NeuralDiffusion  --save_model --eval_interval=40  --lr_decay --n_run=1  --ecp_n_sim=100 --ecp_n_samples=200
